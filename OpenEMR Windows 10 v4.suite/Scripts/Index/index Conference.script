



run "Modules/EMR".Login "admin", "pass"


repeat forever Times  
	put  "Modules/Patient".BuildFromRandom ("1") into  PatientData
	put item repeatindex() of PatientData into PatientInfo
	Run "Patient/New".create PatientInfo as parameters
	Run "Patient/New".ClearCreate
	Click "Chrome/minimize"
	Run "Library/MicroDicom"
	Click "Chrome/Reopen"
	
end repeat
run "Modules/EMR".logout

